extends ../main-layout

block headAppendTitle
    title TJ Tech - FAQ
block scriptFootAppend
    script(src='assets/scripts/faq-detail.js' defer='')


block content
    .container.faq-root
        .row
            .col-12.col-md-4.pt-5
                .text-intro.text-nowrap.mt-5
                    | The Future
                    br
                    |  Of Digital Mastery
                a(href="contact.html")
                    button.btn-base.learn-btn Learn More
            .col-12.col-md-8
                img(style='width: 100%;' src='assets/images/t_community.svg' alt='')
        .g-header.text-center.ecosys-leading(style='margin-top: 102px;') FAQ
        .faq-form
            form.search-form(action='#')
                .faq-search-box
                    input(type='text' placeholder='Find advice and answers from our support team fast or get in touch |')
                    button(type='submit') Search
            .faq-list
                a.faq-item(href='faq-detail.html?q=0')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           About TJ TECH
                a.faq-item(href='faq-detail.html?q=1')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           TJ TECH Customer Resource Management Software
                a.faq-item(href='faq-detail.html?q=2')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           TJ TECH Supportive add-ons and plug-ins
                a.faq-item(href='faq-detail.html?q=3')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           TJ TECH System Maintenance Service
                a.faq-item(href='faq-detail.html?q=4')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           TJ TECH Customer Support Service
                a.faq-item(href='faq-detail.html?q=5')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           Marketing and Promotions
                a.faq-item(href='faq-detail.html?q=6')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           Terms and Policies
                a.faq-item(href='faq-detail.html?q=7')
                    img(src='assets/images/ic_question.svg' alt='')
                    |           Contact Information

